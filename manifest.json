{
  "manifest_version": 3,
  "name": "Weatherility",
  "short_name": "Weatherility",
  "description": "weather forecast without devilry",
  "default_locale": "uk-UA",
  "permissions": [
    "geolocation"
  ],
  "optional_permissions": [
    "unlimitedStorage"
  ],
  "start_url": ".",
  "display": "standalone",
  "background_color": "#282c34",
  "theme_color": "#000000",
  "icons": [{
    "src": "images/touch/homescreen48.png",
    "sizes": "48x48",
    "type": "image/png"
  }, {
    "src": "images/touch/homescreen72.png",
    "sizes": "72x72",
    "type": "image/png"
  }, {
    "src": "images/touch/homescreen96.png",
    "sizes": "96x96",
    "type": "image/png"
  }, {
    "src": "images/touch/homescreen144.png",
    "sizes": "144x144",
    "type": "image/png"
  }, {
    "src": "images/touch/homescreen168.png",
    "sizes": "168x168",
    "type": "image/png"
  }, {
    "src": "images/touch/homescreen192.png",
    "sizes": "192x192",
    "type": "image/png"
  }]
}
